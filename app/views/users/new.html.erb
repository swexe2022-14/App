<%= form_for @user , url: {action: :create} do |f| %>
    <%= f.label :uid, 'ユーザー名' %>
    <%= f.text_field :uid %>
    <%= f.label :pass, 'パスワード' %>
    <%= f.text_field :pass %>
    <%= f.label :code, 'パスコード(数字でのみご入力してください)' %>
    <%= f.number_field :code %>
    
    <%= f.submit '登録' %>
<% end %>